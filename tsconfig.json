{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "esnext",
    "esModuleInterop": true,
    "rootDir": "src",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true
  }
}